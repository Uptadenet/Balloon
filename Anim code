icon.scale.xml

<?xml version="1.0" encoding="utf-8"?>
<set
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:repeatCount="-1"
    android:repeatMode="2">

    <scale
        android:duration="700"
        android:pivotX="fraction(1073741872)"
        android:pivotY="fraction(1073741872)"
        android:repeatCount="-1"
        android:repeatMode="2"
        android:fromXScale="1065353216.000000"
        android:toXScale="1067030912.000000"
        android:fromYScale="1065353216.000000"
        android:toYScale="1067030912.000000" />
</set>

x_in_scale.xml

<?xml version="1.0" encoding="utf-8"?>
