    <translate
        android:duration="1000"
        android:fillAfter="true"
        android:fromYDelta="fraction(257)"
        android:toYDelta="fraction(1)" />

    <translate
        android:duration="2000"
        android:fillAfter="true"
        android:startOffset="1000"
        android:fromXDelta="fraction(1)"
        android:toXDelta="fraction(-255)" />
</set>

